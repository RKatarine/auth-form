    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.5",
    "tailwind-css-variables": "^3.0.1",
    "tailwindcss": "^3.1.6",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.15",
