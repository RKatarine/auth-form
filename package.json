    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.12",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
