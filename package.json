{
  "name": "auth-form",
  "version": "1.0.0",
  "description": "Application for test",
  "engines": {
    "node": "16.x || 18.x"
  },
  "main": "index.js",
  "repository": "git@github.com:RKatarine/auth-form.git",
  "author": "rkate <rkatarine@mail.ru>",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint -c .eslintrc src --ext .js,.jsx,.ts,.tsx",
    "check:unused:exports": "ts-unused-exports ./tsconfig.json --allowUnusedTypes --ignoreFiles=generated",
    "deploy": "yarn build && gh-pages -d build",
    "preview": "vite preview --port 3000",
    "pretty": "prettier \"./**/**.{ts,tsx,js,jsx,mjs,cjs,css,scss,sass,md,*.json,.prettierrc,.lintstagedrc,.babelrc,.eslintrc,*.webmanifest}\"",
    "format": "yarn pretty --write",
    "format:check": "yarn pretty --check",
    "test:e2e": "playwright test",
    "test:e2e-report": "playwright show-report"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
